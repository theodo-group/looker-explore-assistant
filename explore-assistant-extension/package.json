    "@babel/runtime": "^7.25.0",
    "@types/node": "^22.0.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/readable-stream": "^4.0.15",
    "@types/styled-components": "^5.1.34",
    "@types/styled-system": "^5.1.22",
